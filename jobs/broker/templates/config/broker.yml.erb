broker:
  username: starkandwayne
  password: starkandwayne
  port: 8888

router:
  hostname: <%= p("broker.router.hostname") %>

kvstore:
  type: etcd
  machines: <%= p("etcd.machines").map {|h| "http://#{h}:4001"} %>
  username: ""
  password: ""

backends:
<% p("broker.backends.machines").each do |machine| %>
- guid: <%= machine %>
  availability_zone: z1
  uri: <%= machine %>
  username: "<%= p('broker.backends.username') %>"
  password: "<%= p('broker.backends.password') %>"
<% end %>
